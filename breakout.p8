pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--ball variables
ball_y=1
ball_x=33
ball_dx=2
ball_dy=2
ball_r=2
ball_dr=0.5
--misc
frame=0
col=0
--pad variables
pad_x=52
pad_y=120
pad_dx=0
--pad dimensions
pad_w=24
pad_h=3
--pad color
pad_c=7


function _init()
	cls()
end

function _update()
 
 buttpress=false
 
 if btn(0) then
 --left
 	pad_dx=-3
 	buttpress=true
 	--pad_x-=3
 end
 if btn(1) then
 --right
 	pad_dx=3
 	buttpress=true
 	--pad_x+=3
 end
 if not (buttpress) then
 	pad_dx=pad_dx/1.6
 end
 pad_x+=pad_dx
 
 ball_x+=ball_dx
 ball_y+=ball_dy
 
 if ball_x > 127 or ball_x < 0 then
 	ball_dx = -ball_dx
 	sfx(2)
 end
	if ball_y > 127 or ball_y < 0 then
 	ball_dy = -ball_dy
 	sfx(2)
 end
 pad_c=7
 if ball_box(pad_x,pad_y,pad_w,pad_h) then
 	--deal with collision
 pad_c=8
 sfx(1)
 ball_dy=-ball_dy
 end
end

function _draw()
	rectfill(0,0,127,127,2)
	circfill(ball_x,ball_y,ball_r,12)
	rectfill(pad_x,pad_y,pad_x+pad_w,pad_y+pad_h,pad_c)
end

function ball_box(box_x,box_y,box_w,box_h)
	if ball_y-ball_r > box_y+box_h then 
		return false
	end
	if ball_y+ball_r < box_y then 
		return false
	end
	if ball_x-ball_r > box_x+box_w then 
		return false
	end
	if ball_x+ball_r < box_x then 
		return false
	end
	return true
end

